{
  "name": "mineflayer-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for AI agents controlling Minecraft bots",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "start": "node dist/index.js",
    "start:game-agent": "node dist/agent/claude-agent.js",
    "start:mcp-ws": "node dist/mcp-ws-server.js",
    "start:mcp-ws:watch": "nodemon --watch dist/mcp-ws-server.js --watch dist/bot-manager.js --watch dist/tools dist/mcp-ws-server.js",
    "start:dev-agent": "node dist/agent/dev-agent.js",
    "start:self-improve": "node dist/agent/claude-agent.js & node dist/agent/dev-agent.js & node dist/agent/review-agent.js & wait",
    "start:review-agent": "node dist/agent/review-agent.js",
    "start:report-agent": "node dist/agent/report-agent.js",
    "board": "node dist/realtime-board.js",
    "board:view": "node dist/board-viewer.js",
    "start:mamba": "cd /Users/shingo/Develop/mamba-agent && ./run.sh"
  },
  "keywords": [
    "mcp",
    "minecraft",
    "mineflayer",
    "ai",
    "claude"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.29",
    "@anthropic-ai/sdk": "^0.72.1",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@modelcontextprotocol/server-filesystem": "^2026.1.14",
    "canvas": "^3.2.1",
    "chokidar": "^5.0.0",
    "dotenv": "^16.3.1",
    "minecraft-data": "^3.78.0",
    "mineflayer": "^4.20.1",
    "mineflayer-pathfinder": "^2.4.5",
    "node-fetch": "^3.3.2",
    "prismarine-viewer": "^1.33.0",
    "vec3": "^0.1.10",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.18.1",
    "nodemon": "^3.1.11",
    "typescript": "^5.3.0"
  }
}
