{
  "name": "mineflayer-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for visualizing AI agent thinking in Minecraft",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "start": "node dist/index.js",
    "start:agent": "node dist/agent/index.js",
    "start:mcp-ws": "node dist/mcp-ws-server.js"
  },
  "keywords": [
