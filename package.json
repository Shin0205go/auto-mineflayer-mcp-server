    "minecraft",
    "mineflayer",
    "ai",
    "visualization"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "dotenv": "^16.3.1",
    "mineflayer": "^4.20.1",
    "screenshot-desktop": "^1.15.0",
    "sharp": "^0.33.2",
    "vec3": "^0.1.10",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "typescript": "^5.3.0"
  }
}
